  "components": {
    "schemas": {
      "rateshoppingResult": {
        "type": "object",
        "properties": {
          "Rate": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "Carrier": {
                  "type": "string",
                  "description": "Type of carrier",
                  "example": "UPS,FedEx,DHL,Conway"
                },
                "Service": {
                  "type": "string",
                  "description": "Type of service"
                },
                "CarrierServiceCode": {
                  "type": "string",
                  "description": "Carrier service code"
                },
                "PublishedRate": {
                  "type": "number",
                  "description": "Published rate for the shipment"
                },
                "DiscountedRate": {
                  "type": "number",
                  "description": "Discounted rate for the shipment"
                },
                "BaseServiceCharge": {
                  "type": "number",
                  "description": "Base service charge for the shipment"
                },
                "TransportationCharges": {
                  "type": "number",
                  "description": "Transportation charges for the shipment"
                },
